{
  "name": "skillshaker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "eslint:check": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore ./src",
    "eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore ./src --fix",
    "husky-install": "husky install",
    "lint-staged": "lint-staged --allow-empty --config lint-staged.js",
    "lint-staged-fix": "lint-staged --alow-empty --config lint-staged-fix.js",
    "prettier:check": "prettier --check ./src",
    "prettier:fix": "prettier --write ./src"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.3",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "axios": "^1.3.4",
    "bootstrap": "^5.2.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "js-cookie": "^3.0.1",
    "node-sass": "^9.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.9.0",
    "react-toastify": "^9.1.2",
    "sass": "^1.60.0",
    "styled-reset": "^4.4.5",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
